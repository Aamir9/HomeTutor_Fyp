<div class="banner">
    <div class="banner-top">

        <div class="row">
            <div class="col-md-10 frm">


                @using (Html.BeginForm("Search","Home",FormMethod.Post))
                {



                    <div class="form-group">
                        <p class="your-para">Select Your Study Level</p>

                        @Html.DropDownListFor(model => model.LevelOfTaught, taughtlist, "--Select Level Of Taught--", new { @class = "form-control mySelect " })
                    </div>
                    <div class="form-group">
                        <p class="your-para">Select Your City</p>

                        @*@Html.DropDownList("cityId", null, "--select city--", htmlAttributes: new { @class = "form-control",@id= "cityid" })*@

