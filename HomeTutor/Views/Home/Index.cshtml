                    <p class="your-para"> Select Your Subject</p>
                    <div class="form-group">
                        @Html.TextBoxFor(a=>a.Expertise ,null, new { @placeholder = "Select your subject", @class="form-control mySelect"})
                       
                    </div>



                    <div class="form-group m-t-40">
                        <input type="submit" name="name" value="Search" class="btn btn-primary btn-block" />

